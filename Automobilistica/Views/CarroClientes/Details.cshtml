@model Automobilistica.Models.CarroCliente

@{
    ViewData["Title"] = "Details";
}

<h1>Consulta</h1>

<div>
    <h4>Carro do Cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.Label("Data de Cadastro")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ccdtcadastro)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Status")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ccstatus)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Código do Carro")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CccdcarroNavigation.Crcdcarro)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Código do Cliente")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CccdclienteNavigation.Clcdcliente)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { idCar=@Model.CccdcarroNavigation.Crcdcarro, idClie=@Model.CccdclienteNavigation.Clcdcliente }, new { @class = "btn btn-secondary" })
    <a asp-action="Index" class="btn btn-danger" style="margin-left: 10px;">Back to List</a>
</div>
