@model Automobilistica.Models.Pessoas

@{
    ViewData["Title"] = "Details";
}

<h1>Consulta</h1>

<div>
    <h4>Pessoas</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.Label("Nome")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Psnome)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("E-mail")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Psemail)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("CPF/CNPJ")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Pscgc)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Data de Nascimento")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Psdtnascimento)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Data de Cadastro")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Psdtcadastro)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Endereço")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PscdenderecoNavigation.Edbairro)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Pscdpessoa" class="btn btn-primary" style="margin-right: 10px;">Edit</a>
    <a asp-action="Index" class="btn btn-danger">Back to List</a>
</div>
