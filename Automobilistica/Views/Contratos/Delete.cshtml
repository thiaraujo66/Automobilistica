@model Automobilistica.Models.Contrato

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.Label("Data de Cadastro")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ctdtcadastro)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Data de Inicio")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ctdtini)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Data Final")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ctdtfinal)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Total de Horas")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cttotalhoras)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Descricao do Processo")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CtcdprocessoNavigation.Pcdescricao)
        </dd>
        <dt class = "col-sm-2">
            @Html.Label("Código da Proposta")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CtcdpropostaNavigation.Ppcdproposta)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Ctcdcontrato" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
