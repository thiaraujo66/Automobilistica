@model Automobilistica.Models.PropostaServico

@{
    ViewData["Title"] = "Edit";
}

<h1>Alteração</h1>

<h4>Proposta de Serviço</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
