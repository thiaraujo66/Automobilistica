@model Automobilistica.Models.Servicos

@{
    ViewData["Title"] = "Details";
}

<h1>Consulta</h1>

<div>
    <h4>Serviços</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.Label("Nome")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Srnome)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Descrição")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Srdescricao)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Valor")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Srvalor)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Status")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Srstatus)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Data de Cadastro")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Srdtcadastro)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Srcdservico" class="btn btn-primary" style="margin-right: 10px;">Edit</a>
    <a asp-action="Index" class="btn btn-danger">Back to List</a>
</div>
